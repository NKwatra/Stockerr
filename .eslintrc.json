{
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "browser": true,
    "jest": true,
    "es2017": true
  },
  "plugins": ["react", "jsx-a11y", "import"],
  "rules": {
    "no-console": 1,
    "no-dupe-else-if": 1,
    "no-extra-parens": 1,
    "block-scoped-var": 1,
    "no-alert": 1,
    "no-empty-function": 1,
    "no-return-assign": 1,
    "no-undef-init": 1,
    "camelcase": 1,
    "no-inline-comments": 1
  },
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:react/recommended",
    "plugin:jsx-ally/recommended",
    "prettier",
    "prettier/react"
  ]
}
